{
  "name": "glhd/bits",
  "description": "",
  "keywords": [
    "laravel"
  ],
  "authors": [
    {
      "name": "Chris Morrell",
      "homepage": "http://www.cmorrell.com"
    }
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9|^10|11.x-dev|dev-master",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^7.10|^8|9.x-dev|10.x-dev|dev-master",
    "friendsofphp/php-cs-fixer": "^3.5",
    "pestphp/pest": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Glhd\\Bits\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Glhd\\Bits\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "pest": "vendor/bin/pest",
    "fix-style": "vendor/bin/php-cs-fixer fix",
    "check-style": "vendor/bin/php-cs-fixer fix --diff --dry-run"
  },
  "extra": {
    "laravel": {
      "providers": [
          "Glhd\\Bits\\BitsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
